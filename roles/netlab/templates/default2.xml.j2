<network>
  <name>default2</name>
  <forward mode='route'> </forward>
  <bridge name='br2' stp='on' delay='0'/>
  <ip address='{{ netlab_libvirt_default2_dhcp_address }}' netmask='{{ netlab_libvirt_default2_dhcp_mask }}'>
    <dhcp>
      <range start='{{ netlab_libvirt_default2_dhcp_start }}' end='{{ netlab_libvirt_default2_dhcp_end }}'/>
      <host mac='52:54:00:cc:cc:02' ip='{{ netlab_default_ip_ios02 }}'/>
      <host mac='52:54:00:aa:aa:02' ip='{{ netlab_default_ip_veos02 }}'/>
      <host mac='52:54:00:bb:bb:02' ip='{{ netlab_default_ip_junos02 }}'/>
      <host mac='52:54:00:dd:dd:02' ip='{{ netlab_default_ip_centos02 }}'/>
    </dhcp>
  </ip>
</network>

